{
  "name": "coral-reef-theme",
  "displayName": "Coral Reef Theme",
  "description": "As if you were coding in mids of a coral reef...",
  "version": "1.3.2",
  "publisher": "ThePelican",
  "repository": {
    "type": "git",
    "url": "https://github.com/spaceinvader021/coral-reef-theme"
  },
  "icon": "resources/icon.png",
  "keywords": [
    "theme",
    "dark theme",
    "the pelican",
    "thepelican",
    "the pelican dark",
    "blue theme"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Coral Reef",
        "uiTheme": "vs-dark",
        "path": "./themes/Coral Reef-color-theme.json"
      }
    ],
    "grammars": [
      {
        "path": "./syntaxes/injection.json",
        "scopeName": "comment-keywords.injection",
        "injectTo": [
          "source.js",
          "source.cpp",
          "source.c",
          "source.python"
        ]
      }
    ]
  }
}
