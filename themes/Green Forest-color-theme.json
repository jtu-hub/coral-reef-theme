	"tokenColors": [
//////////GENERAL
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0B6E8F"
			}
		},
		{
			"name": "Text",
			"scope": [
				"source",
				"text"
			],
			"settings": {
				"foreground": "#91BDCC"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#CC8564",
			}
		},
		{
			"name": "Control Keywords",
			"scope": [
				"keyword.control",
			],
			"settings": {
				"foreground": "#CCB350",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.separator",
				"punctuation.terminator",
				"punctuation.separator.continuation",
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#b0cAcE",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant.other.color",
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#D9BDB5",
			}
		},	
//////////JS
		{
			"name": "Variables Declaration Keys JS",
